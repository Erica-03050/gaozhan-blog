{
  "crons": [
    {
      "path": "/api/cron/monitor",
      "schedule": "0 20 * * *"
    }
  ],
  "functions": {
    "src/app/api/cron/monitor/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/monitor/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": ""
        },
        {
          "key": "X-Content-Security-Policy",
          "value": ""
        },
        {
          "key": "X-WebKit-CSP",
          "value": ""
        },
        {
          "key": "X-XSS-Protection",
          "value": ""
        }
      ]
    }
  ]
}
